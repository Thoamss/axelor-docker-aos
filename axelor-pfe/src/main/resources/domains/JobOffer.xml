<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">


  <module name="ModuleRH" package="com.axelor.ModuleRH.db"/>

  <entity name="JobOffer" table="offers">

    <string name="title" title="Intitulé de l'offre" max="255"/>
    <many-to-many name="competences" title="Compétences requises" ref="Competences"/>
    <string name="localisation" title="Localisation"/>
    <integer name="xp" title="Expérience requise"/>

  </entity>

</domain-models>