<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">


  <module name="ModuleRH" package="com.axelor.ModuleRH.db"/>

  <entity name="AddressPFE" table="addresspfe">

    <string name="street" title="Rue" max="255"/>
    <string name="city" title="Ville"/>
    <string name="zipcode" title="Code Postal"/>
    <string name="country" title="Pays"/>

  </entity>

</domain-models>