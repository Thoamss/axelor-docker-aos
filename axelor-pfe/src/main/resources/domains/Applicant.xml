<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="ModuleRH" package="com.axelor.ModuleRH.db"/>

  <entity name="Applicant" table="applicant">

    <string name="name" title="Nom" max="255"/>
    <string name="surname" title="Prénom" max="255"/>
    <enum name="title" title="Civilité" ref="TitleEnum"/>
    <string name="mail" title="Adresse Mail" max="255"/>
    <string name="phone" title="N° de téléphone" max="255"/>
    <one-to-one name="address" ref="AddressPFE"/>
    <integer name="experience" title="Années d'expériences"/>
    <many-to-many name="rolesc" title="Rôles" ref="RolesCandidat"/>
    <many-to-many name="competences" title="Compétences" ref="Competences"/>
    <boolean name="open" title="Ouvert"/>
    <many-to-many name="businessSector" title="Secteurs souhaités"
      ref="BusinessSectors"/>
    <string name="localisation" title="Localisation" max="255"/>
    <integer name="workawayMin" title="Taux de télétravail min (en %)"/>
    <integer name="workawayMax" title="Taux de télétravail max (en %)"/>
    <date name="availabilityDate" title="Disponibilité"/>
    <string name="availabilityWait" title="Délai avant embauche"/>
    <many-to-many name="offers" title="Offres d'emploi" ref="JobOffer"/>
    <enum name="typeContrat" title="Type de contrat" ref="ContractTypeEnum"/>
    <string name="salary" title="Prétention salariale (en k€ brut annuel)" max="255"/>
    <string name="comments" title="Commentaires" max="255"/>
    <enum name="status" title="Statut" ref="StatusEnum"/>

  </entity>

</domain-models>
