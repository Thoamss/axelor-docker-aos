<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="ModuleRH" package="com.axelor.ModuleRH.db"/>

  <entity name="Matching" table="matching">

    <string name="title" title="Titre"/>
    <many-to-one name="applicant" title="Candidat" ref="Applicant"/>
    <many-to-one name="opportunity" title="Opportunité" ref="OpportunityPFE"/>
    <integer name="matchRate" title="Taux de Match"/>
    <string name="location" title="Localisation"/>
    <many-to-many name="competences" title="Compétences Recherchées" ref="Competences"/>
    <integer name="marginRate" title="Taux de marge"/>
    <integer name="experience" title="Experience"/>
    <many-to-many name="roles" title="Rôles Recherchés" ref="RolesCandidat"/>

  </entity>

</domain-models>